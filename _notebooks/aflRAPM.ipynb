   "cell_type": "code",
   "execution_count": 161,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "# join to players\n",
    "players2 = players.merge(effects, how='inner', on='playerIndex').sort_values('value', axis=0, ascending=False)"
   ]
