   ]
  },
  {
   "cell_type": "code",
   "execution_count": 160,
   "metadata": {},
   "outputs": [],
   "source": [
    "model = Ridge(alpha=65, fit_intercept=True).fit(x, y)\n",
    "\n",
    "effects = pd.DataFrame(np.expand_dims(np.array(model.coef_.tolist()), axis=1).T, columns=list(range(k))).T.reset_index()\n",
    "effects.columns = ['playerIndex', 'value']\n",
    "effects['playerIndex'] = effects.playerIndex.astype(int)"
   ]
  },
