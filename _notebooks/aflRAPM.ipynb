    "# create a subsetted dataframe for ease of use\n",
    "cols = ['Season', 'Round', 'Home.team', 'Away.team', 'Home.score', 'Away.score', 'HQ4G', 'HQ4B', 'AQ4G', 'AQ4B', 'ID', 'First.name', 'Surname', 'Playing.for', 'Time.on.Ground..']\n",
    "df2 = df[cols].rename(columns={'Home.team':'homeTeam', 'Away.team':'awayTeam', 'Home.score':'homeScore', 'Away.score':'awayScore','Playing.for':'playerTeam', 'ID':'playerId', 'Time.on.Ground..':'timeOnGround', 'First.name':'firstName', 'HQ4G':'HG', 'HQ4B':'HB', 'AQ4G':'AG', 'AQ4B':'AB'})\n",
