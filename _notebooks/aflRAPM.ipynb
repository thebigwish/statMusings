   "source": [
    "# adjust home and away scores for season scoring trends\n",
    "seasonal = df2.groupby('Season')[['xHomePts', 'xAwayPts']].mean().reset_index().rename(columns={'xHomePts':'homeAvg', 'xAwayPts':'awayAvg'})\n",
    "df2 = df2.merge(seasonal, how='inner', on='Season')\n",
    "df2['xHomePtsAdj'] = df2.xHomePts.mean() * (df2.xHomePts / df2.homeAvg)\n",
    "df2['xAwayPtsAdj'] = df2.xAwayPts.mean() * (df2.xAwayPts / df2.awayAvg)\n",
    "df2['xMarginAdj'] = df2.xHomePtsAdj - df2.xAwayPtsAdj\n",
    "df2.head()"
   ]
