   "outputs": [],
   "source": [
    "# generate RAPM x and y matrices\n",
    "mids = df2.matchIndex.tolist()\n",
    "pids = df2.playerIndex.tolist()\n",
    "homes = df2.home.tolist()\n",
    "n, k = matches.shape[0], players.shape[0]\n",
    "\n",
    "x = encodeMatrix(mids, pids, homes, n, k)\n",
    "y = matches.xMarginAdj.values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 159,
   "metadata": {},
   "outputs": [],
   "source": [
    "#model = RidgeCV(cv=3, alphas = np.linspace(1,100,10), fit_intercept=True).fit(x, y)\n",
