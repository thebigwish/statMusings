   "source": [
    "def encodeMatrix(mids, pids, homes, n, k):\n",
    "    arr = np.zeros((n,k))\n",
    "    n = len(mids)\n",
    "    for i in range(n):\n",
    "        mid, pid, home = mids[i], pids[i], homes[i]\n",
    "        arr[mid,pid] = home\n",
    "    return arr"
   ]
  },
